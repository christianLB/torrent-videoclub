{
  "name": "torrent-videoclub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check-env": "node scripts/check-env.js",
    "dev": "npm run check-env && cross-env NEXT_TELEMETRY_DISABLED=1 next dev",
    "dev:win": "npm run check-env && set NEXT_TELEMETRY_DISABLED=1 && next dev --port 3002",
    "dev:clean": "rimraf .next && npm run check-env && cross-env NEXT_TELEMETRY_DISABLED=1 next dev",
    "dev:docker": "docker-compose up",
    "dev:docker:build": "docker-compose up --build",
    "build": "npm run check-env && cross-env NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "cross-env NEXT_TELEMETRY_DISABLED=1 next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.511.0",
    "mongodb": "^6.16.0",
    "next": "^14.2.9",
    "next-themes": "^0.4.6",
    "node-cron": "^4.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.5.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.9",
    "jsdom": "^26.1.0",
    "msw": "^2.3.0",
    "postcss": "^8",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^3.1.4"
  }
}
