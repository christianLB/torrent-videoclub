{
  "name": "torrent-videoclub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check-env": "node scripts/check-env.js",
    "dev": "npm run check-env && cross-env NEXT_TELEMETRY_DISABLED=1 next dev",
    "dev:win": "npm run check-env && set NEXT_TELEMETRY_DISABLED=1 && next dev --port 3002",
    "dev:clean": "rimraf .next && npm run check-env && cross-env NEXT_TELEMETRY_DISABLED=1 next dev",
    "dev:redis": "npm run check-env && concurrently \"redis-server\" \"npm run dev\"",
    "dev:docker": "docker-compose up",
    "dev:docker:build": "docker-compose up --build",
    "build": "npm run check-env && cross-env NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "cross-env NEXT_TELEMETRY_DISABLED=1 next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "ioredis": "^5.6.1",
    "next": "15.1.8",
    "next-themes": "^0.4.6",
    "node-cron": "^4.0.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.5.0",
    "msw": "^2.3.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.1.8",
    "jsdom": "^26.1.0",
    "postcss": "^8",
    "redis-server": "^1.2.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^3.1.4"
  }
}
